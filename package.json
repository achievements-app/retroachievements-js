{
  "version": "0.0.0-development",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && tsdx lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "name": "retroachievements-js",
  "author": "wescopeland",
  "module": "dist/retroachievements-nodejs-api.esm.js",
  "size-limit": [
    {
      "path": "dist/retroachievements-js.cjs.production.min.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@size-limit/preset-small-lib": "^4.9.2",
    "@types/traverse": "^0.6.32",
    "cz-conventional-changelog": "3.3.0",
    "husky": "^4.3.8",
    "msw": "^0.26.2",
    "pretty-quick": "^3.1.0",
    "semantic-release": "^17.3.7",
    "size-limit": "^4.9.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "camelcase-keys": "^6.2.2",
    "isomorphic-unfetch": "^3.1.0",
    "traverse": "^0.6.6",
    "urlcat": "^2.0.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wescopeland/retroachievements-js.git"
  }
}
